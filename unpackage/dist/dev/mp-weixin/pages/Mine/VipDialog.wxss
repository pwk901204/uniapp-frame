.vip-dialog {
  position: fixed;
  width: 100%;
  height: 100%;
}
.vip-dialog .vip-dialog-bg {
    position: absolute;
    width: 100%;
    height: 100%;
    background-color: #000000;
    opacity: 0.4;
    z-index: 9997;
}
.vip-dialog .vip-dialog-content {
    position: fixed;
    top: 30%;
    left: 0;
    width: 100%;
    z-index: 9999;
}
.vip-dialog .vip-dialog-content .vip-dialog-box {
      position: relative;
      margin: 0 auto;
      width: 604rpx;
      height: 464rpx;
      padding: 40rpx;
      box-sizing: border-box;
      background-color: #FFFFFF;
      border-radius: 20rpx;
}
.vip-dialog .vip-dialog-content .vip-dialog-box .dialog-close {
        position: absolute;
        top: 10rpx;
        right: 20rpx;
        width: 26rpx;
        height: 26rpx;
}
.vip-dialog .vip-dialog-content .vip-dialog-box .dialog-close image {
        width: 100%;
        height: 100%;
}
.vip-dialog .vip-dialog-content .vip-dialog-box .vip-dilog-title {
        color: #333333;
        font-size: 32rpx;
        line-height: 32rpx;
        padding-bottom: 30rpx;
        font-weight: bold;
}
.vip-dialog .vip-dialog-content .vip-dialog-box .vip-dilog-title .text-light {
          color: #c89f53;
}
.vip-dialog .vip-dialog-content .vip-dialog-box .vip-dilog-title2 {
        color: #333333;
        font-size: 28rpx;
        line-height: 28rpx;
        padding-bottom: 30rpx;
        margin-top: 30rpx;
}
.vip-dialog .vip-dialog-content .vip-dialog-box .vip-dilog-list .vip-dilog-item {
        -webkit-flex-grow: 1;
                flex-grow: 1;
        text-align: center;
        color: #333333;
        font-size: 22rpx;
        line-height: 22rpx;
}
.vip-dialog .vip-dialog-content .vip-dialog-box .vip-dilog-list .vip-dilog-item image {
        width: 100rpx;
        height: 100rpx;
}
.vip-dialog .vip-dialog-content .vip-dialog-box .vip-dilog-list .vip-dilog-item text {
        padding-top: 30rpx;
}

