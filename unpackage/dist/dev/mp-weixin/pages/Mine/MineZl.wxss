/*=================注册第一步=========================*/
.regis-form{
	padding-top: 39rpx;
}
.regis-page-posi{
	position: relative;
	padding-bottom: 120rpx;
}
.regis-page-nobtn{
	padding-bottom: 0;
}
.regis-form .regis-input-item{
	margin-bottom: 30rpx;
	padding: 0 30rpx;
	height: 88rpx;
	line-height: 88rpx;
	-webkit-align-items: center;
	        align-items: center;
	background-color: #ebebf0;
	border-radius: 10rpx;
}
.regis-form .regis-textarea{
	width: 100%;
	padding: 20rpx;
	box-sizing: border-box;
	height: 270rpx;
	font-size: 28rpx;
	line-height: 1.4;
	color: #666666;
	z-index: 0;
}
.regis-form .regis-input-item-small{
	-webkit-align-items: center;
	        align-items: center;
	font-size: 24rpx;
}
.regis-form .regis-input-item-small .check-box{
	position: relative;
	width: 230rpx;
	color: #222222;
}
.regis-form .regis-input-item-small .check-box checkbox{
	visibility: hidden;
	width: 28rpx;
	height: 28rpx;
	margin-right: 20rpx;
}
.regis-form .regis-input-item-small .check-box image{
	position: absolute;
	left: 0;
	top: 6rpx;
	width: 28rpx;
	height: 28rpx;
}
.regis-form .regis-input-item .input-label{
	padding: 0 10rpx;
	font-size: 28rpx;
	line-height: 34rpx;
	color: #222222;
}
.regis-form .regis-input-item .input-label .require{
	color: #ff5050;
}
.regis-form .regis-input-item .input-label .not-require{
	color: #ff5050;
	visibility: hidden;
}
.regis-form .regis-input-item .input-label.label-yzm{
	border-left: 1px solid #c1c1c1;
	color: #3167d9;
}
.regis-form .regis-input-item .input-label.label-yzm.disable{
	border-left: 1px solid #c1c1c1;
	color: #666666;
}
.regis-form .regis-input-item input{
	background-color: #ebebf0;
	font-size: 28rpx;
	color: #222222;
}
.regis-form .regis-input-item-small .agree-notice{
	color: #3167d9;
}
.regis-page-posi .regis-btn {
	background-color: #e4e5e7;
	padding-top: 22rpx;
	position: fixed;
	width: 100%;
	bottom: 0;
	margin-left: -30rpx;
}
.regis-btn2{
	background-color: #e4e5e7;
	position: fixed;
	width: 100%;
	bottom: 0;
}
.regis-btn2 button{
	margin-top: 20rpx;
	margin-bottom: 0;
	border-radius: 0;
}
.regis-page-posi .regis-btn button{
	margin-bottom: 0;
	border-radius: 0;
}
.regis-page-audit2{
