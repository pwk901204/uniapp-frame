.regis-page{
	padding: 0 28rpx;
}
.regis-page .regis-chose-box{
	padding-top: 98rpx;
}
.regis-page button{
	margin-bottom: 40rpx;
	height: 88rpx;
	line-height: 88rpx;
	border-radius: 88rpx;
	font-size: 34rpx;
	color: #FFFFFF;
}
.regis-page button image{
	/* position: relative; */
