}
.regis-page .full-input-right{
	width: 0;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
	text-align: right;
	padding-left: 25rpx;
	font-size: 28rpx;
	color: #666666;
}
.regis-page .icon-right{
	display: inline-block;
	margin-left: 21rpx;
	padding-right: 25rpx;
	width: 12rpx;
	height: 26rpx;
}
.regis-page .regis-picker{
	display: inline-block;
	width: 0;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
	text-align: right;
	font-size: 28rpx;
	color: #666666;
}
.regis-page .regis-picker .uni-input{
	padding-left: 10rpx;
	padding-right: 10rpx;
}
.regis-page .uni-input{
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.regis-pd{
	padding: 0 28rpx;
}
.regis-top{
	padding: 20rpx 28rpx;
}
.regis-top .flex_row{
	border: 1px solid #3167d9;
	border-radius: 10rpx;
	line-height: 70rpx;
	height: 70rpx;
}
.regis-top-item{
	text-align: center;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
	color: #3167d9;
	font-size: 30rpx;
	line-height: 70rpx;
	border-right: 1px solid #3167d9;
}
.regis-top-item.active{
	background-color: #3167d9;
	color: #FFFFFF;
}
.regis-top-item:last-child{
	border-right: none;
}
.regis-tab{
	border-top: 1px solid #f3f3f3;
	border-bottom: 1px solid #f3f3f3;
}
.regis-tab .flex_row{
	height: 89rpx;
}
.regis-tab-item{
	width: 0;
	-webkit-flex-grow: 1;
	        flex-grow: 1;
	text-align: center;
}
.regis-tab-item text{
	line-height: 85rpx;
	display: inline-block;
	padding-right: 10rpx;
	box-sizing: border-box;
	border-bottom: 4px solid transpant;
}
.regis-tab-item.active text{
	border-bottom: 4px solid #3167d9;
}
.regis-scroll-box .scroll-Y{
}
.regis-radio-box {
	text-align: right;
	padding: 0 20rpx;
}
.regis-radio-item{
	display: inline-block;
	width: 100rpx;
	font-size: 28rpx;
	color: #666666;
}
.regis-radio-item .regis-radio{
	position:  relative;
}
.regis-radio-item .regis-radio radio{
	opacity: 0;
}
.regis-radio-item .regis-radio image{
	position: absolute;
	left: 10rpx;
	top: 10rpx;
	width: 32rpx;
	height: 32rpx;
	z-index: 0;
}
