<view class="regis-form"><view class="flex_row regis-input-item"><view class="input-label"><label class="require">*</label><text>融资轮次</text></view><picker class="regis-picker" value="{{rzlcIndex}}" disabled="{{disabled}}" range-key="mc" range="{{rzlcList}}" data-event-opts="{{[['change',[['bindRzlcChange',['$event']]]]]}}" bindchange="__e"><block wx:if="{{rzlcIndex!==-1}}"><view class="uni-input">{{rzlcList[rzlcIndex].mc}}</view></block><block wx:else><view class="uni-input">请选择</view></block></picker><view><image class="icon-right" mode="aspectFit" src="../../../static/Regis/icon-right.png"></image></view></view><view class="flex_row regis-input-item"><view class="input-label"><label class="require">*</label><text>融资时间</text></view><picker class="regis-picker" mode="date" fields="month" disabled="{{disabled}}" value="{{formdata.time}}" start="{{startDate}}" end="{{endDate}}" data-event-opts="{{[['change',[['bindrzsjChange',['$event']]]]]}}" bindchange="__e"><block wx:if="{{formdata.time}}"><view class="uni-input">{{formdata.time}}</view></block><block wx:else><view class="uni-input">请选择</view></block></picker><view><image class="icon-right" mode="aspectFit" src="../../../static/Regis/icon-right.png"></image></view></view><view class="flex_row regis-input-item"><view class="input-label"><label class="require">*</label><text>投资机构</text></view><input class="uni-input regis-input" name="organization" disabled="{{disabled}}" placeholder="请填写" data-event-opts="{{[['input',[['__set_model',['$0','organization','$event',[]],['formdata']]]]]}}" value="{{formdata.organization}}" bindinput="__e"/></view><view class="flex_row regis-input-item"><view class="input-label"><label class="require not-require">*</label><text>融资金额</text></view><input class="uni-input regis-input" name="money" disabled="{{disabled}}" placeholder="(万/人民币)" data-event-opts="{{[['input',[['__set_model',['$0','money','$event',[]],['formdata']]]]]}}" value="{{formdata.money}}" bindinput="__e"/></view></view>