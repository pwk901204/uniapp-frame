.container{min-height:100vh}.search_result{box-sizing:border-box;padding-top:220rpx}.search_result .head{position:fixed;top:0;left:0;width:100%;z-index:100}.search_result .search{background:#fff;padding:20rpx 30rpx}.search_result .search .search_wrap{width:620rpx;height:60rpx;background:#f5f5f5;border-radius:30rpx}.search_result .search .search_wrap icon{margin:0 15rpx 0 20rpx}.search_result .search .search_wrap input{width:520rpx}.search_result .search>text{font-size:28rpx;color:#3167d9}.search_result .tab_wrap{background:#f3f3f3;padding:20rpx 30rpx}.search_result .tab_wrap .tab{border:1px solid #3167d9;border-radius:8rpx}.search_result .tab_wrap .tab view{width:33.3%;color:#3167d9;font-size:30rpx;text-align:center;line-height:70rpx}.search_result .tab_wrap .tab view:nth-of-type(2){border-left:1px solid #3167d9;border-right:1px solid #3167d9}.search_result .tab_wrap .tab view.active{background:#3167d9;color:#fff}.search_result .no_data{position:absolute;left:50%;top:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}.search_result .no_data image{width:356rpx;height:232rpx}.search_result .no_data text{font-size:28rpx;color:#666;margin-top:50rpx}.search_result .search_list{padding:0 30rpx;box-sizing:border-box}.search_result .search_list.bg_white{background:#fff;padding:30rpx}.investor_status{position:absolute;right:20rpx;top:25rpx;height:40rpx;width:150rpx;border:1px solid #f89225;color:#f89225;font-size:26rpx;text-align:center;line-height:37rpx;border-radius:5px}.investor_status.send{color:#31a6e1;border-color:#31a6e1}.investor_status.each{color:#aaa;border-color:#aaa}.investor_dec{-webkit-flex-wrap:wrap;flex-wrap:wrap;margin-top:20rpx}.investor_dec .w50{width:50%}.investor_dec view{padding-right:10rpx;margin-bottom:10rpx;box-sizing:border-box;width:100%}.investor_dec image{width:30rpx;height:30rpx;margin-right:20rpx}.investor_dec text{width:calc(100% - 50rpx);font-size:26rpx;color:#666}