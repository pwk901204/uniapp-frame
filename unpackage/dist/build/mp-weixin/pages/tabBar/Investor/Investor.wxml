<view class="container investor"><view class="search flex_row_justify_center"><view class="search_wrap flex_row_justify_center flex_auto"><image class="flex_none" src="../../../static/icon/search.png"></image><input class="flex_auto" type="text" placeholder="投资人" data-event-opts="{{[['input',[['__set_model',['$0','userName','$event',[]],['params']]]]]}}" value="{{params.userName}}" bindinput="__e"/></view><text data-event-opts="{{[['tap',[['search',['$event']]]]]}}" class="search_btn flex_none" bindtap="__e">搜索</text><image class="filter flex_none" src="../../../static/icon/filter.png" data-event-opts="{{[['tap',[['goFilter',['$event']]]]]}}" bindtap="__e"></image></view><view class="search_list"><block wx:if="{{list.length>0}}"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><investor-item vue-id="{{'1-'+__i0__}}" itemData="{{item}}" bind:__l="__l" vue-slots="{{['status','dec']}}"><view slot="status"><block wx:if="{{false}}"><view class="investor_status">回赠名片</view></block></view><view slot="dec"><view class="investor_dec flex_row"><view class="flex_row_justify_center"><image src="../../../static/project/project_7.png"></image><text class="ell">{{item.company}}</text></view><view class="flex_row_justify_center w50"><image src="../../../static/project/project_8.png"></image><text class="ell">{{item.job}}</text></view><view class="flex_row_justify_center w50"><image src="../../../static/project/project_2.png"></image><text class="ell">{{item.provinceCity}}</text></view><view class="flex_row_justify_center"><image src="../../../static/project/project_1.png"></image><text class="ell">{{item.investmentFieldTxt}}</text></view></view></view></investor-item></block></block><block wx:else><view class="no_data flex_col_center"><image src="../../../static/bg/nodata.png"></image><text>暂无搜索结果</text></view></block><block wx:if="{{showLoadMore}}"><view class="uni-loadmore">{{loadMoreText}}</view></block></view></view>