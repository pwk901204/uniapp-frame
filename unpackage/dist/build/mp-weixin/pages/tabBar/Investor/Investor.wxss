.container{min-height:100vh}.investor{box-sizing:border-box;padding-top:142rpx}.investor .search{position:fixed;top:0;left:0;width:100%;background:#fff;box-sizing:border-box;padding:20rpx 30rpx 30rpx 30rpx;z-index:100}.investor .search .search_wrap{padding:0 10rpx;box-sizing:border-box;height:65rpx;background:#f5f5f5;border-radius:32rpx}.investor .search .search_wrap input{color:#b2b2b2;font-size:26rpx}.investor .search .search_wrap image{width:40rpx;height:40rpx;margin:0 10rpx}.investor .search .search_btn{margin:0 10rpx 0 20rpx;line-height:1;color:#3167d9}.investor .search .filter{width:40rpx;height:40rpx;margin-left:20rpx}.investor .no_data{position:absolute;left:50%;top:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}.investor .no_data image{width:356rpx;height:232rpx}.investor .no_data text{font-size:28rpx;color:#666;margin-top:50rpx}.investor .search_list{padding:0 30rpx;box-sizing:border-box}.investor_status{position:absolute;right:20rpx;top:25rpx;height:40rpx;width:150rpx;border:1px solid #f89225;color:#f89225;font-size:26rpx;text-align:center;line-height:37rpx;border-radius:5px}.investor_status.send{color:#31a6e1;border-color:#31a6e1}.investor_status.each{color:#aaa;border-color:#aaa}.investor_dec{-webkit-flex-wrap:wrap;flex-wrap:wrap;margin-top:20rpx}.investor_dec .w50{width:50%}.investor_dec view{padding-right:10rpx;margin-bottom:10rpx;box-sizing:border-box;width:100%}.investor_dec image{width:30rpx;height:30rpx;margin-right:20rpx}.investor_dec text{width:calc(100% - 50rpx);font-size:26rpx;color:#666}