.pro_filter{background:#fff;padding:0 30rpx 98rpx;position:relative;box-sizing:border-box}.pro_filter .title{font-size:28rpx;line-height:88rpx;color:#333;margin-top:11px}.pro_filter .title text{color:#666;margin-left:12rpx}.pro_filter .title_first{margin:0}.pro_filter .filter_list{-webkit-flex-wrap:wrap;flex-wrap:wrap}.pro_filter .filter_list text{width:216rpx;height:100rpx;font-size:26rpx;color:#666;text-align:center;margin-bottom:20rpx;margin-right:20rpx;border-radius:8px;line-height:1.2;padding:10rpx;box-sizing:border-box}.pro_filter .filter_list text:nth-of-type(3n){margin-right:0}.pro_filter .filter_list text.active{background:#3167d9;color:#fff}.pro_filter .btnWrap{width:100%;position:fixed;left:0;bottom:0;font-size:34rpx}.pro_filter .btnWrap .reset{width:50%;color:#000;text-align:center;line-height:98rpx;background:#fff;box-shadow:-31px 0 15px -20px hsla(0,0%,83.5%,.3) inset}.pro_filter .btnWrap .confirm{width:50%;background:#3167d9;color:#fff;text-align:center;line-height:98rpx}