.container{background:#f3f3f3;min-height:100vh}.project{box-sizing:border-box;padding-top:100rpx}.project .head{position:fixed;top:0;left:0;width:100%;z-index:100}.project .tab_wrap{padding:0 30rpx 20rpx 30rpx;background:#fff}.project .tab_wrap .tab{border:1px solid #3167d9;border-radius:8rpx}.project .tab_wrap .tab view{width:50%;color:#3167d9;font-size:30rpx;text-align:center;line-height:70rpx}.project .tab_wrap .tab view:nth-of-type(2){border-left:1px solid #3167d9}.project .tab_wrap .tab view.active{background:#3167d9;color:#fff}.project .no_data{position:absolute;left:50%;top:50%;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}.project .no_data image{width:374rpx;height:283rpx}.project .no_data text{font-size:28rpx;color:#666}.project .search_list{padding:20rpx 30rpx 0;overflow:auto;box-sizing:border-box;min-height:100%}